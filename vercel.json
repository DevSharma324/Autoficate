{
  "version": 2,
  "builds": [
    {
      "src": "autoficate/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "autoficate/wsgi.py"
    }
  ],
  "build": {
    "commands": [
      {
        "command": "python manage.py collectstatic --noinput"
      }
    ]
  }
}
